{
  "name": "samtheq-com",
  "private": true,
  "author": {
    "name": "Samuel Qui√±ones",
    "email": "samuelquinones@samtheq.com"
  },
  "scripts": {
    "dev": "next dev -p 5555",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next export",
    "build:full": "npm run build && npm run export",
    "start": "next start -p 6006",
    "start:static": "serve out -p 6006",
    "eslint": "eslint --ext js,ts,tsx",
    "lint:next": "next lint --dir src",
    "lint": "npm run eslint src && prettier --check \"src/styles/**/*\"",
    "format": "npm run eslint -- --fix src && prettier --write \"src/styles/**/*\"",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "generate:hook": "npm run generate -- hook",
    "generate:theme": "npm run generate -- theme",
    "please": "prisma migrate dev --name initial-migration --create-only"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@prisma/client": "3.12.0",
    "@restart/ui": "1.2.0",
    "classnames": "2.3.1",
    "dayjs": "1.11.1",
    "focus-trap-react": "8.10.0",
    "framer-motion": "6.3.1",
    "next": "12.1.5",
    "next-seo": "5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/inquirer-autocomplete-prompt": "1.3.4",
    "@types/node": "16.11.27",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/tailwindcss": "3.0.10",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "autoprefixer": "10.4.4",
    "cross-env": "7.0.3",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "inquirer-autocomplete-prompt": "2.0.0",
    "node-plop": "0.26.2",
    "plop": "2.7.6",
    "postcss": "8.4.12",
    "postcss-import": "14.1.0",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "prisma": "3.12.0",
    "serve": "13.0.2",
    "tailwindcss": "3.0.24",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  }
}
