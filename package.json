{
  "name": "samtheq-com",
  "private": true,
  "author": {
    "name": "Samuel Qui√±ones",
    "email": "samuelquinones@samtheq.com"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev -p 5555",
    "dev:debug": "dotenv -v NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p 6006",
    "eslint": "eslint --ext js,ts,tsx",
    "lint:next": "next lint --dir src",
    "lint": "npm run eslint src && prettier --check \"src/styles/**/*\"",
    "format": "npm run eslint -- --fix src && prettier --write \"src/styles/**/*\"",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "generate:hook": "npm run generate -- hook",
    "generate:theme": "npm run generate -- theme",
    "generate:page": "npm run generate -- page",
    "generate:blog": "npm run generate -- blog-post",
    "generate:tag": "npm run generate -- tag",
    "prisma:pull": "dotenv -c development prisma db pull",
    "prisma:seed": "dotenv -c development prisma db seed",
    "prisma:generate": "dotenv -c development prisma generate",
    "prisma:migrate:reset": "dotenv -c development prisma migrate -- reset",
    "prisma:migrate:start": "dotenv -c development prisma migrate -- dev --create-only",
    "prisma:migrate:finish": "dotenv -c development prisma migrate -- dev",
    "prisma:migrate:deploy": "dotenv -c staging prisma migrate -- deploy",
    "prisma:studio:dev": "dotenv -c development prisma studio -- --port 9999",
    "prisma:studio:prod": "dotenv -c production prisma studio -- --port 9999"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.2",
    "@fortawesome/free-brands-svg-icons": "6.1.2",
    "@fortawesome/free-regular-svg-icons": "6.1.2",
    "@fortawesome/free-solid-svg-icons": "6.1.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@prisma/client": "4.5.0",
    "@restart/ui": "1.4.0",
    "classnames": "2.3.2",
    "contentlayer": "0.2.8",
    "dayjs": "1.11.6",
    "framer-motion": "7.6.2",
    "next": "12.3.1",
    "next-contentlayer": "0.2.8",
    "next-seo": "5.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-pretty-code": "0.4.0",
    "rehype-slug": "5.0.1",
    "sharp": "0.31.1",
    "shiki": "0.11.1",
    "swr": "1.3.0",
    "unist-util-remove": "3.1.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.7",
    "@types/node": "16.11.43",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "autoprefixer": "10.4.13",
    "dotenv-cli": "5.1.0",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "inquirer-autocomplete-prompt": "3.0.0",
    "inquirer-datepicker-prompt": "0.4.3",
    "inquirer-directory": "2.2.0",
    "node-plop": "0.26.2",
    "plop": "2.7.6",
    "postcss": "8.4.18",
    "postcss-import": "15.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prisma": "4.5.0",
    "slug": "8.2.2",
    "tailwindcss": "3.1.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
