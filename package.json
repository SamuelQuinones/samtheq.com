{
  "name": "samtheq-com",
  "private": true,
  "author": {
    "name": "Samuel Qui√±ones",
    "email": "samuelquinones@samtheq.com"
  },
  "scripts": {
    "dev": "next dev -p 5555",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next export",
    "build:full": "npm run build && npm run export",
    "start": "next start -p 6006",
    "start:static": "serve out -p 6006",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint src && prettier --check 'src/styles/**/*'",
    "format": "npm run eslint -- --fix src && prettier --write 'src/styles/**/*'",
    "generate": "cross-env TS_NODE_PROJECT=\"./.scripts/ts-node.tsconfig.json\" plop --plopfile .scripts/generators/plopfile.ts",
    "generate:hook": "npm run generate -- hook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-brands-svg-icons": "6.0.0",
    "@fortawesome/free-regular-svg-icons": "6.0.0",
    "@fortawesome/free-solid-svg-icons": "6.0.0",
    "@fortawesome/react-fontawesome": "0.1.17",
    "@popperjs/core": "2.11.2",
    "classnames": "2.3.1",
    "focus-trap-react": "8.9.2",
    "framer-motion": "6.2.8",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-popper": "2.2.5"
  },
  "devDependencies": {
    "@types/node": "16.11.22",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "autoprefixer": "10.4.2",
    "cross-env": "7.0.3",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "node-plop": "0.26.2",
    "plop": "2.7.6",
    "postcss": "8.4.8",
    "postcss-import": "14.0.2",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "0.1.8",
    "serve": "13.0.2",
    "tailwindcss": "3.0.23",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
